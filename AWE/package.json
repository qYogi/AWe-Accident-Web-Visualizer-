{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run build:minify",
    "build:client": "tsc",
    "build:minify": "esbuild public/js/main.js public/js/charts.js public/js/displayAccidents.js public/js/filters.js public/js/map.js public/js/uiHelpers.js public/js/exportTable.js --bundle --minify --format=esm --outdir=public/js --allow-overwrite",
    "dev": "tsc && tsx watch src/server.ts",
    "start": "npm run build && node public/js/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.5.0",
    "leaflet": "^1.9.4",
    "pg": "^8.16.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.18",
    "@types/pg": "^8.15.2",
    "tsx": "^4.19.4"
  }
}
